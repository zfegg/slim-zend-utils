{
    "name": "zfegg/slim-zend-utils",
    "description": "Using Zend components in Slim",
    "keywords": ["microframework", "rest", "router", "psr7"],
    "homepage": "https://github.com/zfegg/slim-zend-utils",
    "type": "library",
    "require": {
        "zendframework/zend-servicemanager": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "squizlabs/php_codesniffer": "^3.0",
        "psr/http-message": "^1.0"
    },
    "license": "MIT",
    "autoload-dev": {
        "psr-4": {
            "ZfeggTest\\ZendUtils\\": "test/"
        }
    },
    "autoload": {
        "psr-4": {
            "Zfegg\\ZendUtils\\": "src/"
        }
    }
}
